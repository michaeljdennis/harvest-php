{
    "name": "michaeljdennis/harvest-api-php",
    "description": "A PHP package for the Harvest time tracking API.",
    "keywords": ["harvest"],
    "license": "MIT",
    "authors": [
        {
            "name": "Michael Dennis",
            "email": "michaeljdennis@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "guzzlehttp/guzzle": "^6.3",
        "vlucas/phpdotenv": "^2.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Harvest\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ],
        "psr-4": {
            "Harvest\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
